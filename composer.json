{
    "name": "nitropack/magento2-extension",
    "description": "The official NitroPack extension for Magento 2.3 and Magento 2.4.",
    "require": {
        "composer-plugin-api": "^2.0",
        "nitropack/nitropackcloud-sdk": "^0.53.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "nitropack/nitropackcloud-sdk": "^0.53.0"
    },
    "type": "composer-plugin",
    "keywords": [
        "magento2",
        "extension",
        "nitropack",
        "performance",
        "speed",
        "cwv",
        "cloud",
        "cache",
        "ecommerce",
        "cdn"
    ],
    "version": "2.1.0",
    "license": "MIT",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "NitroPack\\NitroPack\\": ""
        }
    },
    "scripts": {
        "pre-package-uninstall": [
            "NitroPack\\NitroPack\\Observer\\ComposerScript::composerUninstall"
        ]
    },
    "extra": {
        "class": "NitroPack\\NitroPack\\ComposerUninstall",
        "class-name": "My\Package\ClassName",
        "class-arguments": [
            "arg1",
            "arg2",
            "arg3"
        ]
    }
}
